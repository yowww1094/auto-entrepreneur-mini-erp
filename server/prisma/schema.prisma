generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

enum TypeActivite{
  COMMERCE
  PRESTATION_SERVICE
  MIXTE
}

model AutoEntrepreneur{
  id String @id @default(uuid())
  email String 
  password String
  nom String
  prenom String
  telephone String
  address String
  typeActivite TypeActivite
  tauxImposition Float
  ice String
  logo String
  dateCreation DateTime @default(now())
  dateMiseAJour DateTime @updatedAt
}
